type Publicacion {
    id: ID
    requisicionId: ID
    plataforma: String
    fechaPublicacion: String
    estado: String
}

type Query {
    obtenerPublicaciones: [Publicacion]
}

type Mutation {
    crearPublicacion(requisicionId: ID!, plataforma: String!, fechaPublicacion: String!): Publicacion
}
